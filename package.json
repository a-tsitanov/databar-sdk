{
  "name": "databar-sdk",
  "version": "1.0.0",
  "description": "databar sdk",
  "main": "lib/index.js",
  "type": "module",
  "scripts": {
    "build": " tsc -p .",
    "test": "npm run build && node --experimental-specifier-resolution=node test.js",
    "prettier:base": "prettier --parser typescript --single-quote",
    "prettier:check": "npm run prettier:base -- --list-different \"src/**/*.ts\"",
    "prettier:write": "npm run prettier:base -- --write \"src/**/*.ts\""
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "src/**/*.ts": [
      "npm run prettier:write",
      "git add"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/blendtale/images-map"
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@types/dataframe-js": "^1.4.0",
    "@types/lodash": "^4.14.168",
    "dotenv": "^16.0.1",
    "husky": "^7.0.1",
    "lint-staged": "^10.2.10",
    "prettier": "^2.0.5",
    "typescript": "^4.1.3"
  },
  "dependencies": {
    "axios": "^0.21.1",
    "dataframe-js": "^1.4.4",
    "lodash": "^4.17.20",
    "tslint": "^6.1.3"
  }
}
